{
  "name": "players-feedback-backend",
  "version": "1.0.0",
  "description": "Players feedback application",
  "main": "index.js",
  "scripts": {
    "start": "node build/index.js",
    "build": "npm run clean && npm run migrate && babel -d ./build ./server",
    "dev": "nodemon --exec babel-node server/index.js",
    "clean": "rm -rf build && mkdir build",
    "test": "node test.js",
    "format": "prettier --write \"./**/*.{js,json}\"",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "undo:migrate": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "author": "Igwe Chinonso",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "gulp": "^4.0.2",
    "prettier": "^2.2.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "core-js": "^3.9.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.4",
    "nodemon": "^2.0.7",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "regenerator-runtime": "^0.13.7",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0"
  },
  "engines": {
    "node": "14.x"
  }
}